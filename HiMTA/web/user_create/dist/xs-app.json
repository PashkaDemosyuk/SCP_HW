{
 "welcomeFile":"/index.html",
 "authenticationMethod":"route",
 "routes":[
  {
   "source":"^(.*)$",
   "target":"$1",
   "service":"html5-apps-repo-rt",
   "csrfProtection":true,
   "authenticationType":"xsuaa",
   "scope":{
    "GET":[
     "$XSAPPNAME.HiMTA.view"
    ],
    "POST":[
     "$XSAPPNAME.HiMTA.edit"
    ],
    "PUT":[
     "$XSAPPNAME.HiMTA.edit"
    ],
    "DELETE":[
     "$XSAPPNAME.HiMTA.edit"
    ],
    "default":"$XSAPPNAME.HiMTA.view"
   }
  }
 ]
}