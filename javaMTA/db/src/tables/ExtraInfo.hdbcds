namespace javaDPA;

using javaDPA::Company;

context ExtraInfo{

		entity Office {
    		@Comment: 'Id of Office'
		    key adid : Integer64 generated always as identity ( start with 1 increment by 1 );
		    @Comment: 'Company for Office'
		    compid : String(4);
		    @Comment: 'City'
		    city : String(100);
		    @Comment: 'Street'
		    strt : String(100);
		    @Comment: 'House Number'
		    hnum : Integer;
		};
};